{
	"name": "angular-extended-builders-workspace",
	"version": "21.0.0",
	"license": "MIT",
	"type": "module",
	"private": true,
	"scripts": {
		"watch": "pnpm --parallel --filter=!app watch",
		"build": "pnpm --parallel --filter=!app build",
		"build-app": "ANGULAR_EXTENDED_BUILDER=debug pnpm --filter=app build",
		"serve": "pnpm --filter=app serve",
		"check": "biome check .",
		"format": "biome check --unsafe --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-angular": "^20.0.0",
		"@release-it/bumper": "^7.0.5",
		"@types/node": "^24.10.1",
		"del-cli": "^7.0.0",
		"lefthook": "^2.0.4",
		"prettier": "^3.7.3",
		"release-it": "^19.0.6",
		"typescript": "5.9.3"
	},
	"engines": {
		"node": ">=20.11.1"
	}
}
